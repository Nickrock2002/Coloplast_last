<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/itns_fragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:minWidth="50dp"
    android:orientation="vertical">

    <androidx.cardview.widget.CardView
        android:id="@+id/cvItnsTherapy"
        android:layout_width="120dp"
        android:layout_height="36dp"
        app:cardCornerRadius="5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/guideline2"
        app:layout_constraintTop_toTopOf="@+id/tvItnsTherapy">

        <Spinner
            android:id="@+id/ddItnsTherapy"
            android:layout_width="120dp"
            android:layout_height="36dp"
            android:background="@color/colorControlNoChange" />
    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/tvItnsStimProgress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/all_neuro_in_progress"
        android:textSize="18sp"
        android:visibility="invisible"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvItnsTestLabel"
        app:layout_constraintEnd_toStartOf="@+id/guideline1"
        app:layout_constraintStart_toStartOf="@+id/guideline1" />

    <TextView
        android:id="@+id/tvItnsTherapyLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:text="@string/itns_schedule"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/tvItnsDateLabel"
        app:layout_constraintEnd_toStartOf="@+id/guideline1"
        app:layout_constraintTop_toBottomOf="@+id/guideline5" />

    <TextView
        android:id="@+id/tvItnsAmplitude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:minWidth="75dp"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvItnsAmplitudeRO"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/guideline2" />

    <TextView
        android:id="@+id/tvItnsDateLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:text="@string/itns_date"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/tvItnsTimeLabel"
        app:layout_constraintEnd_toStartOf="@+id/guideline1"
        app:layout_constraintTop_toBottomOf="@+id/tvItnsTherapyLabel" />

    <TextView
        android:id="@+id/tvItnsTimeLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:text="@string/itns_time"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/tvItnsAmplitudeLabel"
        app:layout_constraintEnd_toStartOf="@+id/guideline1"
        app:layout_constraintTop_toBottomOf="@+id/tvItnsDateLabel" />

    <TextView
        android:id="@+id/tvItnsAmplitudeLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:text="@string/all_amplitude"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/guideline6"
        app:layout_constraintEnd_toStartOf="@+id/guideline1"
        app:layout_constraintTop_toBottomOf="@+id/tvItnsTimeLabel" />

    <TextView
        android:id="@+id/tvItnsBtStatus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/no_link_msg"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/cvItnsInterrogate"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintStart_toStartOf="@+id/guideline2"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvItnsModelNumberLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:text="@string/itns_model"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/tvItnsSNLabel"
        app:layout_constraintEnd_toStartOf="@+id/guideline1"
        app:layout_constraintTop_toBottomOf="@+id/guideline4" />

    <TextView
        android:id="@+id/tvItnsSNLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:text="@string/itns_sn"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/tvItnsLeadILabel"
        app:layout_constraintEnd_toStartOf="@+id/guideline1"
        app:layout_constraintTop_toBottomOf="@+id/tvItnsModelNumberLabel" />

    <TextView
        android:id="@+id/tvItnsSN"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:minWidth="50dp"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvItnsSNLabel"
        app:layout_constraintStart_toStartOf="@+id/guideline1" />

    <TextView
        android:id="@+id/tvItnsLeadILabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:text="@string/all_lead_i"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/divider"
        app:layout_constraintEnd_toStartOf="@+id/guideline1"
        app:layout_constraintTop_toBottomOf="@+id/tvItnsSNLabel" />

    <TextView
        android:id="@+id/tvItnsLeadI"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:minWidth="50dp"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvItnsLeadILabel"
        app:layout_constraintStart_toStartOf="@+id/guideline1" />

    <TextView
        android:id="@+id/tvItnsCellV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:fitsSystemWindows="false"
        android:minWidth="50dp"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvItnsCellVLabel"
        app:layout_constraintStart_toStartOf="@+id/guideline3" />

    <TextView
        android:id="@+id/tvItnsCellVLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:text="@string/itns_cell_v"
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvItnsModelNumber"
        app:layout_constraintEnd_toStartOf="@+id/guideline3" />

    <TextView
        android:id="@+id/tvItnsRRT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:minWidth="50dp"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvItnsRRTLabel"
        app:layout_constraintStart_toStartOf="@+id/guideline3" />

    <TextView
        android:id="@+id/tvItnsRRTLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:text="@string/itns_rrt"
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvItnsSN"
        app:layout_constraintEnd_toStartOf="@+id/guideline3" />

    <TextView
        android:id="@+id/tvItnsLeadR"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:minWidth="50dp"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvItnsLeadRLabel"
        app:layout_constraintStart_toStartOf="@+id/guideline3" />

    <TextView
        android:id="@+id/tvItnsLeadRLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:text="@string/all_lead_r"
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvItnsLeadI"
        app:layout_constraintEnd_toStartOf="@+id/guideline3" />

    <TextView
        android:id="@+id/tvItnsModelNumber"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:minWidth="50dp"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvItnsModelNumberLabel"
        app:layout_constraintStart_toStartOf="@+id/guideline1" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.25" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/tvItnsModelNumberLabel"
        app:layout_constraintGuide_percent="0.8"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintGuide_percent="0.25171"
        app:layout_constraintTop_toBottomOf="@+id/tvItnsLeadILabel" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.4514364" />

    <TextView
        android:id="@+id/tvItnsTherapy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:minWidth="50dp"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvItnsTherapyLabel"
        app:layout_constraintStart_toStartOf="@+id/guideline1" />

    <TextView
        android:id="@+id/tvItnsDateRO"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:minWidth="50dp"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvItnsDateLabel"
        app:layout_constraintStart_toStartOf="@+id/guideline1" />

    <TextView
        android:id="@+id/tvItnsTimeRO"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:minWidth="50dp"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvItnsTimeLabel"
        app:layout_constraintStart_toStartOf="@+id/guideline1" />

    <TextView
        android:id="@+id/tvItnsAmplitudeRO"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:minWidth="50dp"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvItnsAmplitudeLabel"
        app:layout_constraintStart_toStartOf="@+id/guideline1" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.75" />


    <androidx.constraintlayout.widget.Group
        android:id="@+id/ghITNS"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:constraint_referenced_ids="tvItnsAmplitude,tvItnsDate,ibItnsPlus,tvItnsAmplitudeLabel,tvItnsTherapyLabel,tvItnsTimeLabel,tvItnsTherapy,tvItnsAmplitudeRO,ddItnsTherapy,tvItnsDateRO,tvItnsTime,ibItnsMinus,tvItnsDateLabel,tvItnsTimeRO,tvItnsCellVLabel,tvItnsLeadRLabel,tvItnsSN,tvItnsModelNumberLabel,tvItnsLeadILabel,tvItnsSNLabel,tvItnsModelNumber,tvItnsRRTLabel,tvItnsLeadI,tvItnsCellV,tvItnsLeadR,tvItnsRRT,divider2,divider,divider3,tvItnsTestLabel,btItnsStartStim,cvItnsStartStim,cvItnsMinus,cvItnsPlus,cvItnsTime,cvItnsTherapy,cvItnsDate"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline4"
        tools:ignore="NotSibling" />

    <View
        android:id="@+id/divider"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintBottom_toTopOf="@+id/guideline5"
        app:layout_constraintTop_toTopOf="@+id/guideline5"
        tools:layout_editor_absoluteX="8dp" />

    <View
        android:id="@+id/divider2"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintBottom_toTopOf="@+id/guideline4"
        tools:layout_editor_absoluteX="4dp" />

    <View
        android:id="@+id/divider3"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintBottom_toTopOf="@+id/guideline6"
        app:layout_constraintTop_toTopOf="@+id/guideline6"
        tools:layout_editor_absoluteX="-31dp" />

    <ProgressBar
        android:id="@+id/pbItns"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="600dp"
        android:layout_height="21dp"
        android:indeterminate="true"
        app:layout_constraintBottom_toTopOf="@+id/cvItnsInterrogate"
        app:layout_constraintEnd_toStartOf="parent"
        app:layout_constraintStart_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvItnsBtStatus" />

    <TextView
        android:id="@+id/tvItnsTestLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/external_test_neuromodulation"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/cvItnsStartStim"
        app:layout_constraintEnd_toStartOf="@+id/guideline3"
        app:layout_constraintStart_toStartOf="@+id/guideline3"
        app:layout_constraintTop_toBottomOf="@+id/guideline6" />

    <ProgressBar
        android:id="@+id/pbItnsStim"
        style="?android:attr/progressBarStyle"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:indeterminate="true"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/cvItnsStartStim"
        app:layout_constraintEnd_toStartOf="@+id/guideline1"
        app:layout_constraintStart_toStartOf="@+id/guideline1"
        app:layout_constraintTop_toTopOf="@+id/cvItnsStartStim" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cvItnsInterrogate"
        android:layout_width="150dp"
        android:layout_height="75dp"
        android:layout_marginEnd="50dp"
        app:cardCornerRadius="5dp"
        app:layout_constraintBottom_toTopOf="@+id/divider2"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintTop_toBottomOf="@+id/tvItnsBtStatus">

        <Button
            android:id="@+id/btItnsInterrogate"
            android:layout_width="150dp"
            android:layout_height="75dp"
            android:background="@color/colorPrimary"
            android:text="@string/itns_btn_interrogate"
            android:textColor="@android:color/white"
            android:textSize="18sp" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cvItnsProgram"
        android:layout_width="150dp"
        android:layout_height="75dp"
        android:layout_marginStart="50dp"
        app:cardCornerRadius="5dp"
        app:layout_constraintBottom_toBottomOf="@+id/cvItnsInterrogate"
        app:layout_constraintStart_toStartOf="@+id/guideline2"
        app:layout_constraintTop_toTopOf="@+id/cvItnsInterrogate">

        <Button
            android:id="@+id/btItnsProgram"
            android:layout_width="150dp"
            android:layout_height="75dp"
            android:background="@color/colorPrimary"
            android:text="@string/itns_btn_program"
            android:textColor="@android:color/white"
            android:textSize="18sp" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cvItnsStartStim"
        android:layout_width="250dp"
        android:layout_height="75dp"
        app:cardCornerRadius="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/guideline3"
        app:layout_constraintStart_toStartOf="@+id/guideline3"
        app:layout_constraintTop_toBottomOf="@+id/tvItnsTestLabel">

        <Button
            android:id="@+id/btItnsStartStim"
            android:layout_width="250dp"
            android:layout_height="75dp"
            android:layout_below="@+id/textView"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:background="@color/colorPrimary"
            android:text="@string/all_btn_start"
            android:textColor="@android:color/white"
            android:textSize="18sp" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cvItnsLink"
        android:layout_width="36dp"
        android:layout_height="36dp"
        android:layout_marginEnd="24dp"
        app:cardCornerRadius="5dp"
        app:layout_constraintBottom_toBottomOf="@+id/tvItnsBtStatus"
        app:layout_constraintEnd_toStartOf="@+id/tvItnsBtStatus"
        app:layout_constraintTop_toTopOf="@+id/tvItnsBtStatus">

        <ImageView
            android:id="@+id/ivItnsLink"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:background="@color/colorControlNoChange"
            android:visibility="visible"
            app:srcCompat="@drawable/ic_link_off"
            tools:visibility="visible"
            tools:ignore="ContentDescription" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cvItnsMinus"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginEnd="24dp"
        app:cardCornerRadius="5dp"
        app:layout_constraintBottom_toBottomOf="@+id/tvItnsAmplitude"
        app:layout_constraintEnd_toStartOf="@+id/tvItnsAmplitude"
        app:layout_constraintTop_toTopOf="@+id/tvItnsAmplitude">

        <ImageButton
            android:id="@+id/ibItnsMinus"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:background="@color/colorControlNoChange"
            app:srcCompat="@drawable/ic_minus"
            tools:ignore="ContentDescription" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cvItnsPlus"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="24dp"
        app:cardCornerRadius="5dp"
        app:layout_constraintBottom_toBottomOf="@+id/tvItnsAmplitude"
        app:layout_constraintStart_toEndOf="@+id/tvItnsAmplitude"
        app:layout_constraintTop_toTopOf="@+id/tvItnsAmplitude">

        <ImageButton
            android:id="@+id/ibItnsPlus"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:background="@color/colorControlNoChange"
            app:srcCompat="@drawable/ic_plus"
            tools:ignore="ContentDescription" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cvItnsTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:cardCornerRadius="5dp"
        app:layout_constraintBottom_toBottomOf="@+id/tvItnsTimeRO"
        app:layout_constraintEnd_toStartOf="@+id/guideline3"
        app:layout_constraintStart_toStartOf="@+id/guideline3"
        app:layout_constraintTop_toTopOf="@+id/tvItnsTimeRO">

        <TextView
            android:id="@+id/tvItnsTime"
            android:layout_width="wrap_content"
            android:layout_height="36dp"
            android:background="@color/colorControlNoChange"
            android:clickable="true"
            android:focusable="true"
            android:gravity="center"
            android:minWidth="120dp"
            android:textColor="@android:color/black"
            android:textSize="18sp" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cvItnsDate"
        android:layout_width="120dp"
        android:layout_height="36dp"
        app:cardCornerRadius="5dp"
        app:layout_constraintBottom_toBottomOf="@+id/tvItnsDateRO"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/guideline2"
        app:layout_constraintTop_toTopOf="@+id/tvItnsDateRO">

        <TextView
            android:id="@+id/tvItnsDate"
            android:layout_width="wrap_content"
            android:layout_height="36dp"
            android:background="@color/colorControlNoChange"
            android:clickable="true"
            android:focusable="true"
            android:gravity="center"
            android:minWidth="120dp"
            android:textColor="@android:color/black"
            android:textSize="18sp" />
    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>